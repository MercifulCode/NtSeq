{
  "name": "nt-seq-ts",
  "version": "0.0.2",
  "description": "Nucleotide sequence manipulation and analysis library",
  "author": "Keith Horwood",
  "contributors": [
    {
      "name": "Drew Diamantoukos",
      "email": "MercifulCode@gmail.com",
      "url": "https://github.com/MercifulCode"
    }
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "tslint --project ./",
    "precommit": "lint-staged && yarn test",
    "prepublish": "rm -rf lib && yarn build && git add lib && git add -u lib",
    "test": "jest"
  },
  "files": [
    "lib/",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "nucleotide",
    "nucleic",
    "acid",
    "dna",
    "rna",
    "sequence",
    "bioinformatics",
    "biotechnology",
    "computational",
    "biology",
    "analysis",
    "genomics",
    "gene",
    "biojs",
    "typescript"
  ],
  "lint-staged": {
    "ignore": [
      "lib"
    ],
    "linters": {
      "*.{md,ts}": [
        "prettier --write",
        "git add"
      ],
      "*.md": [
        "markdownlint -c ./markdownlint.json",
        "git add"
      ],
      "*.{ts}": [
        "tslint",
        "git add"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MercifulCode/NtSeq"
  },
  "devDependencies": {
    "@types/jest": "^23.3.0",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "lint-staged": "^7.2.0",
    "markdownlint-cli": "^0.11.0",
    "prettier": "^1.13.7",
    "ts-jest": "^23.0.1",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-jsdoc-rules": "^0.1.2",
    "typescript": "^2.9.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  },
  "dependencies": {
    "@types/node": "^10.5.2"
  }
}
